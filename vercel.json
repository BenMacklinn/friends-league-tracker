{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python@3.9"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/leaderboard" },
    { "source": "/leaderboard", "destination": "/api/leaderboard" },
    { "source": "/battles/recent", "destination": "/api/battles" }
  ]
}
